  li+li+li+li+li+li+li+li {
    -xv-voice-volume: 7%;
  }
  li+li+li+li+li+li+li+li+li {
    -xv-voice-volume: 8%;
  }
  li+li+li+li+li+li+li+li+li+li {
    -xv-voice-volume: 9%;
  }
  li+li+li+li+li+li+li+li+li+li+li {
    -xv-voice-volume: 10%;
  }
  li+li+li+li+li+li+li+li+li+li+li+li {
