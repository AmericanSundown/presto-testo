</xsl:choose>
</result>
<xsl:if test="false()">
<zeta token="attribute-value" xml:id="id1"><omega xml:lang="no-nb" xml:id="id2"><nu insert="solid 1px green" xml:lang="no" xml:id="id3"><upsilon><iota desciption="solid 1px green" xml:id="id4"><alpha content="false" xml:lang="en-US" xml:id="id5"/><any attrib="another attribute value" xml:id="id6"/><xi attr="content" xml:lang="en" xml:id="id7"/><iota><pi xml:lang="en" xml:id="id8"/><zeta true="attribute" xml:lang="en-GB" xml:id="id9"/><psi content="false" xml:lang="no-nb"/><any xml:id="id10"><alpha xml:lang="en-US" xml:id="id11"><epsilon content="this.nodeValue" xml:id="id12"><green>This text must be green</green></epsilon></alpha></any></iota></iota></upsilon></nu></omega></zeta></xsl:if>
</xsl:template>
</xsl:stylesheet>
