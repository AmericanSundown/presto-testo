</xsl:choose>
</result>
<xsl:if test="false()">
<kappa insert="100%" xml:lang="en-GB" xml:id="id1"><pi xml:lang="no" xml:id="id2"><zeta xml:lang="en-US"><tau xml:id="id3"/><delta token="this.nodeValue"><phi/><alpha><pi xml:lang="no-nb"/><eta xml:lang="en" xml:id="id4"/><tau xml:lang="en-US" xml:id="id5"><mu desciption="100%"/><any xml:lang="en-GB"><chi string="attribute-value" xml:id="id6"><green>This text must be green</green></chi></any></tau></alpha></delta></zeta></pi></kappa></xsl:if>
</xsl:template>
</xsl:stylesheet>
