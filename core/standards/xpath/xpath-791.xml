<xsl:if test="false()">
<rho src="false" xml:id="id1"><nu att="_blank" xml:lang="no"><gamma token="123456789"/><zeta attr="attribute value"><nu xml:lang="en-GB" xml:id="id2"/><psi><alpha xml:lang="no"/><tau xml:lang="nb" xml:id="id3"><iota/><kappa true="this.nodeValue" xml:lang="no" xml:id="id4"><green>This text must be green</green></kappa></tau></psi></zeta></nu></rho></xsl:if>
</xsl:template>
</xsl:stylesheet>
