<xi data="_blank" xml:lang="en"><omicron or="false" xml:lang="en-US"><phi object="100%" xml:lang="en-US" xml:id="id1"/><psi insert="true"/><epsilon false="attribute value" xml:lang="no" xml:id="id2"><sigma xml:id="id3"><delta><omega false="true" xml:lang="en-US"><delta and="123456789"><green>This text must be green</green></delta></omega></delta></sigma></epsilon></omicron></xi></xsl:if>
