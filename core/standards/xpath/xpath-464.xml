<alpha class="_blank" xml:lang="en-GB" xml:id="id1"><zeta string="attribute-value" xml:id="id2"/><psi xml:lang="en-GB" xml:id="id3"/><epsilon xml:lang="en-GB"/><sigma attribute="attribute" xml:lang="no" xml:id="id4"/><beta xml:lang="no-nb"><chi data="false" xml:id="id5"><tau xml:lang="no" xml:id="id6"><upsilon xml:lang="no"/><chi class="this.nodeValue" xml:lang="nb"/><psi xml:id="id7"/><psi xml:lang="no"/><psi name="_blank" xml:lang="no" xml:id="id8"><green>This text must be green</green></psi></tau></chi></beta></alpha></xsl:if>
