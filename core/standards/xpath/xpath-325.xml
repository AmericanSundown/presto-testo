<eta attr="100%" xml:lang="en"><chi string="attribute-value" xml:id="id1"><epsilon xml:lang="nb"/><gamma attrib="attribute" xml:lang="en"/><pi><nu xml:lang="nb"><iota xml:lang="nb"/><tau xml:lang="no"/><kappa data="attribute value" xml:lang="no-nb"><xi xml:lang="nb"/><lambda xml:lang="no" xml:id="id2"><xi xml:id="id3"><nu name="true" xml:lang="en-GB"><green>This text must be green</green></nu></xi></lambda></kappa></nu></pi></chi></eta></xsl:if>
