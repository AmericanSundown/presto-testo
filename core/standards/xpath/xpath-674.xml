<eta xml:lang="nb"><pi abort="attribute value" xml:lang="en-US"><any string="attribute"><delta xml:lang="en-GB"/><iota class="123456789" xml:id="id1"><rho><beta xml:lang="en-US"><any xml:id="id2"><beta xml:id="id3"/><gamma src="_blank"/><epsilon object="attribute" xml:lang="en-GB"><kappa content="attribute-value" xml:lang="nb"/><nu att="solid 1px green"><delta xml:lang="en-US" xml:id="id4"/><pi xml:lang="no-nb"><iota attribute="attribute" xml:id="id5"/><upsilon false="attribute-value" xml:id="id6"/><kappa delete="this.nodeValue" xml:id="id7"><green>This text must be green</green></kappa></pi></nu></epsilon></any></beta></rho></iota></any></pi></eta></xsl:if>
