<beta xml:lang="no-nb" xml:id="id1"><tau xml:lang="nb" xml:id="id2"/><psi xml:id="id3"><xi false="false" xml:id="id4"/><kappa><lambda><alpha xml:id="id5"><phi att="_blank"/><gamma/><upsilon and="this.nodeValue" xml:id="id6"><omega xml:lang="no" xml:id="id7"><green>This text must be green</green></omega></upsilon></alpha></lambda></kappa></psi></beta></xsl:if>
