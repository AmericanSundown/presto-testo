<xsl:if test="false()">
<phi xml:id="id1"><psi true="solid 1px green" xml:id="id2"/><omega xml:lang="en-US"/><chi src="content" xml:id="id3"><tau name="123456789"><chi xml:lang="nb" xml:id="id4"/><eta xml:lang="no"/><phi object="this-is-att-value" xml:lang="en" xml:id="id5"/><delta number="solid 1px green" xml:lang="no"/><sigma insert="100%" xml:id="id6"><chi att="123456789"/><eta number="attribute-value" xml:lang="en-GB" xml:id="id7"><theta string="123456789" xml:lang="en"><kappa><xi data="this.nodeValue" xml:lang="no"><pi attr="100%"><green>This text must be green</green></pi></xi></kappa></theta></eta></sigma></tau></chi></phi></xsl:if>
