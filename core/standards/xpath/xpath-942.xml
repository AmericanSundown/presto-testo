<pi xml:id="id1"><tau xml:lang="nb" xml:id="id2"/><upsilon xml:lang="en-GB" xml:id="id3"/><psi><pi xml:lang="no"/><omega string="attribute" xml:lang="en-GB"/><mu xml:lang="en" xml:id="id4"/><tau insert="_blank" xml:lang="en-GB" xml:id="id5"><xi number="123456789" xml:lang="no-nb"><epsilon xml:lang="no"/><beta number="_blank" xml:id="id6"/><beta src="123456789" xml:lang="en-US" xml:id="id7"><xi xml:lang="no-nb"><epsilon xml:lang="en-GB" xml:id="id8"><beta xml:id="id9"/><lambda attribute="attribute-value"><green>This text must be green</green></lambda></epsilon></xi></beta></xi></tau></psi></pi></xsl:if>
