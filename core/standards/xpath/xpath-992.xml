</xsl:choose>
</result>
<xsl:if test="false()">
<mu abort="_blank"><eta xml:id="id1"/><omega data="attribute value"/><any xml:id="id2"><tau class="attribute-value" xml:lang="en-US" xml:id="id3"/><pi delete="123456789" xml:lang="nb" xml:id="id4"><tau xml:lang="en-US" xml:id="id5"><delta xml:lang="nb" xml:id="id6"><beta xml:lang="no" xml:id="id7"/><alpha><xi xml:id="id8"><xi abort="solid 1px green" xml:lang="no" xml:id="id9"/><psi insert="123456789"/><nu string="100%"/><gamma number="_blank" xml:lang="en-US" xml:id="id10"/><phi><green>This text must be green</green></phi></xi></alpha></delta></tau></pi></any></mu></xsl:if>
