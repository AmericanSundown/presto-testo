<gamma content="this.nodeValue"><lambda xml:lang="no-nb"><upsilon xml:lang="en"><beta xml:lang="en-GB"><omicron xml:id="id1"/><upsilon xml:lang="no" xml:id="id2"><kappa xml:lang="en"><beta xml:lang="en-GB"/><nu xml:lang="en-GB"><xi/><sigma xml:lang="en" xml:id="id3"><sigma attr="this.nodeValue" xml:lang="en-GB" xml:id="id4"/><mu desciption="true" xml:id="id5"><tau xml:id="id6"/><delta token="attribute-value" xml:lang="en-US" xml:id="id7"><omicron xml:lang="en" xml:id="id8"><green>This text must be green</green></omicron></delta></mu></sigma></nu></kappa></upsilon></beta></upsilon></lambda></gamma></xsl:if>
