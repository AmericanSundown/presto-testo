</xsl:choose>
</result>
<xsl:if test="false()">
<delta title="_blank"><tau xml:lang="nb" xml:id="id1"/><omicron xml:lang="en-US"/><tau xml:id="id2"><lambda object="attribute-value"/><iota xml:lang="en-US" xml:id="id3"/><xi false="false" xml:lang="en-US"/><epsilon number="attribute" xml:lang="en-US" xml:id="id4"/><nu src="attribute value" xml:lang="en-US" xml:id="id5"><xi xml:id="id6"/><upsilon src="this.nodeValue" xml:id="id7"><chi xml:lang="en-GB" xml:id="id8"><green>This text must be green</green></chi></upsilon></nu></tau></delta></xsl:if>
</xsl:template>
</xsl:stylesheet>
