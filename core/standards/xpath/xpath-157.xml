<any xml:lang="en-US"><psi xml:id="id1"><tau xml:lang="no"><omicron attr="true" xml:lang="nb"/><epsilon delete="content"/><theta xml:lang="en-US" xml:id="id2"/><tau><phi/><beta true="attribute value" xml:id="id3"/><pi xml:lang="en-US"><xi/><rho content="another attribute value"/><chi xml:id="id4"/><gamma><theta xml:id="id5"><xi xml:lang="en-US" xml:id="id6"/><pi abort="this-is-att-value" xml:lang="no" xml:id="id7"/><delta><kappa token="100%" xml:lang="en-US"/><iota abort="123456789" xml:id="id8"><green>This text must be green</green></iota></delta></theta></gamma></pi></tau></tau></psi></any></xsl:if>
