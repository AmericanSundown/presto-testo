</xsl:choose>
</result>
<xsl:if test="false()">
<beta data="attribute" xml:lang="en-US" xml:id="id1"><lambda attr="_blank" xml:lang="en-US" xml:id="id2"/><iota or="another attribute value" xml:lang="en-GB" xml:id="id3"/><eta and="false" xml:lang="en-GB"><epsilon or="123456789" xml:id="id4"/><epsilon attribute="this-is-att-value"><zeta content="this.nodeValue" xml:lang="nb" xml:id="id5"><any xml:id="id6"/><gamma attrib="content" xml:id="id7"/><eta xml:id="id8"><green>This text must be green</green></eta></zeta></epsilon></eta></beta></xsl:if>
</xsl:template>
</xsl:stylesheet>
