<xsl:if test="false()">
<kappa><kappa false="content" xml:id="id1"/><iota xml:lang="en"/><psi att="attribute value"><sigma xml:lang="en" xml:id="id2"/><delta xml:lang="nb"><xi name="true"/><lambda token="100%" xml:lang="en-GB"/><beta xml:id="id3"/><upsilon attr="solid 1px green" xml:lang="en-US"><green>This text must be green</green></upsilon></delta></psi></kappa></xsl:if>
