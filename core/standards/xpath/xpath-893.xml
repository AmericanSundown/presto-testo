</xsl:choose>
</result>
<xsl:if test="false()">
<kappa delete="attribute value" xml:id="id1"><gamma name="false" xml:lang="nb" xml:id="id2"/><mu/><kappa delete="attribute" xml:lang="no-nb" xml:id="id3"/><iota class="this.nodeValue" xml:id="id4"/><any xml:id="id5"><delta object="another attribute value" xml:id="id6"><iota content="another attribute value" xml:lang="en-GB" xml:id="id7"/><pi xml:lang="no-nb" xml:id="id8"><phi content="this.nodeValue" xml:id="id9"/><lambda xml:lang="no-nb"><iota and="solid 1px green"><green>This text must be green</green></iota></lambda></pi></delta></any></kappa></xsl:if>
</xsl:template>
</xsl:stylesheet>
