	<xsl:value-of select="'PASS'"/>
</xsl:when>
<xsl:otherwise>FAIL</xsl:otherwise>
</xsl:choose>
</result>
<xsl:if test="false()">
<mu and="false"><any content="attribute value" xml:lang="en-US" xml:id="id1"/><epsilon xml:lang="no"/><zeta content="another attribute value" xml:id="id2"><mu name="false" xml:lang="no-nb"/><sigma/><gamma xml:lang="no"><upsilon xml:id="id3"><omicron att="solid 1px green" xml:lang="en"/><psi xml:lang="en-GB" xml:id="id4"/><pi xml:lang="no-nb" xml:id="id5"/><gamma xml:lang="en"><gamma att="solid 1px green"><epsilon attrib="true"/><sigma xml:lang="en-GB"/><chi abort="attribute value" xml:lang="en-GB" xml:id="id6"/><omega xml:id="id7"><nu xml:lang="en"><green>This text must be green</green></nu></omega></gamma></gamma></upsilon></gamma></zeta></mu></xsl:if>
