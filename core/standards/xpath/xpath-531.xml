	<xsl:value-of select="'PASS'"/>
</xsl:when>
<xsl:otherwise>FAIL</xsl:otherwise>
</xsl:choose>
</result>
<xsl:if test="false()">
<epsilon or="attribute"><sigma class="another attribute value"/><rho xml:lang="no" xml:id="id1"/><psi insert="true" xml:id="id2"/><rho and="_blank" xml:lang="no" xml:id="id3"><tau xml:lang="en-GB" xml:id="id4"/><kappa xml:lang="en-GB"><kappa number="attribute value" xml:lang="en-GB" xml:id="id5"><beta><any xml:lang="en-GB" xml:id="id6"><sigma xml:lang="en-US" xml:id="id7"/><sigma xml:lang="en"><omega xml:lang="en-US"/><omega xml:id="id8"><psi xml:id="id9"/><pi data="100%" xml:lang="en" xml:id="id10"><zeta abort="content" xml:id="id11"/><beta xml:lang="en"/><any insert="false"><green>This text must be green</green></any></pi></omega></sigma></any></beta></kappa></kappa></rho></epsilon></xsl:if>
