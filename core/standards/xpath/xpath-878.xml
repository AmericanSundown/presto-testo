</xsl:choose>
</result>
<xsl:if test="false()">
<pi xml:lang="no"><epsilon xml:lang="en"/><iota xml:lang="en-US"><phi xml:id="id1"/><tau xml:lang="en-US"/><eta/><tau xml:id="id2"/><omicron abort="attribute" xml:lang="no"/><sigma><rho string="another attribute value" xml:id="id3"/><pi name="content"/><upsilon xml:lang="nb" xml:id="id4"><zeta data="false" xml:lang="nb"/><xi true="true" xml:lang="en-GB"/><beta class="this.nodeValue" xml:lang="nb"><epsilon insert="123456789" xml:lang="nb" xml:id="id5"/><theta xml:lang="no"><eta token="attribute" xml:lang="no-nb"><theta xml:lang="en-GB" xml:id="id6"><gamma attr="_blank" xml:lang="en-US" xml:id="id7"/><sigma class="attribute" xml:id="id8"><pi false="this.nodeValue"/><theta xml:id="id9"><green>This text must be green</green></theta></sigma></theta></eta></theta></beta></upsilon></sigma></iota></pi></xsl:if>
