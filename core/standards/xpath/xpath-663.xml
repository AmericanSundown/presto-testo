</xsl:choose>
</result>
<xsl:if test="false()">
<phi xml:lang="en-US"><psi src="attribute value" xml:id="id1"><zeta/><pi abort="this-is-att-value" xml:id="id2"><sigma xml:lang="en-US" xml:id="id3"/><omega xml:lang="en-GB"/><theta xml:lang="en-US" xml:id="id4"><eta attrib="false"><upsilon xml:id="id5"/><lambda true="this.nodeValue" xml:lang="no"/><kappa desciption="100%" xml:id="id6"/><omicron attrib="attribute-value"><any and="true"/><omicron number="attribute-value" xml:lang="nb"/><eta xml:lang="nb" xml:id="id7"><green>This text must be green</green></eta></omicron></eta></theta></pi></psi></phi></xsl:if>
</xsl:template>
</xsl:stylesheet>
