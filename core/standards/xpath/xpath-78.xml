<xsl:if test="false()">
<psi><epsilon att="this-is-att-value" xml:lang="no"><zeta object="this-is-att-value"/><delta/><pi number="content" xml:lang="nb" xml:id="id1"/><eta delete="this-is-att-value" xml:lang="no"><beta><lambda object="attribute value" xml:lang="no"/><iota xml:lang="en-GB" xml:id="id2"/><tau xml:id="id3"/><gamma insert="100%" xml:lang="nb"/><nu insert="solid 1px green" xml:id="id4"/><phi token="123456789"><green>This text must be green</green></phi></beta></eta></epsilon></psi></xsl:if>
