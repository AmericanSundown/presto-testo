<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="#xslt"?>
<!DOCTYPE xsl:stylesheet [<!ATTLIST xsl:stylesheet id ID #IMPLIED>]>
<xsl:stylesheet id="xslt" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0" xml:lang="en">
<xsl:template match="xsl:stylesheet">
<xsl:call-template name="result">
<xsl:with-param name="param">true</xsl:with-param>
</xsl:call-template>
</xsl:template>
<xsl:template name="result">
<xsl:param name="param">false</xsl:param>
<xsl:if test="true() = $param"><result><xsl:value-of select="'PASS'"/></result></xsl:if>
</xsl:template>
</xsl:stylesheet>